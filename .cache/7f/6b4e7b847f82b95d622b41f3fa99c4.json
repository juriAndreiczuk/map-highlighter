lass MapCanvasRect extends MapCanvas {\n  constructor(props) {\n    super(props);\n    this.getInfo()\n    this.canvas.onmousemove = (e) => {\n      this.mapHilight(e)\n    }\n  }\n\n  getInfo() {\n    for(const area of this.areas) {\n      const co =  area.coords.split(',');\n      this.info.coords.push({\n        x: co[0], y: co[1], w: co[2] - co[0], h: co[3] - co[1]\n      })\n      this.info.hrefs.push(area.href);\n    }\n  }\n\n  mapHilight(e) {\n    const rect = this.canvas.getBoundingClientRect();\n    const x = e.clientX - rect.left;\n    const y = e.clientY - rect.top;\n    this.ctx.clearRect(0, 0, this.canvas.width, this.canvas.height);\n    for(let i = 0; i < this.info.coords.length; i++) {\n      const coords = this.info.coords[i];\n      this.ctx.beginPath(this.ctx.isPointInPath(x, y));\n      this.ctx.rect(coords.x, coords.y, coords.w, coords.h);\n      this.ctx.fillStyle = this.ctx.isPointInPath(x, y) ?  'transparent' : this.hoverColor;\n      this.ctx.fill();\n      if(this.ctx.isPointInPath(x, y)) {\n        this.canvas.onclick = () => {\n          location.href = this.info.hrefs[i]\n        }\n      }\n    }\n  }\n}\n\nexport default MapCanvasRect\n"},"lineCount":null}},"error":null,"hash":"4ae8a10a6e6f95aef86976a19868ac01","cacheData":{"env":{}}}